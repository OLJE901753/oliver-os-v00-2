{
  "name": "oliver-os-frontend",
  "version": "0.0.2",
  "description": "Oliver-OS Frontend - AI-Brain Interface",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "start": "vite --host",
    "build": "tsc --project tsconfig.frontend.json && vite build",
    "preview": "vite preview",
