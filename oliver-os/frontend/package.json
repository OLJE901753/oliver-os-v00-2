    "type-check": "tsc --project tsconfig.frontend.json --noEmit"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.17.0",
    "clsx": "^2.0.0",
    "cytoscape": "^3.27.0",
    "d3": "^7.8.5",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.20.1",
    "socket.io-client": "^4.7.5",
    "tailwind-merge": "^2.2.0",
    "three": "^0.159.0",
    "y-indexeddb": "^9.0.11",
    "y-websocket": "^1.5.0",
    "yjs": "^13.6.10",
    "zustand": "^4.4.7"
